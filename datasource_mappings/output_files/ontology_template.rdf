<owl:Class rdf:about="CloseState">
  <rdfs:subClassOf rdf:resource="OpenCloseState"/>
  <rdfs:label xml:lang="en">Close state</rdfs:label>
  <rdfs:comment xml:lang="en">The state of a device that is CLOSE</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="OpenCloseState">
  <rdfs:subClassOf rdf:resource="State"/>
  <rdfs:label xml:lang="en">Open close state</rdfs:label>
  <rdfs:comment xml:lang="en">A type of state</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="Property">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom rdf:resource="Measurement"/>
      <owl:onProperty rdf:resource="relatesToMeasurement"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom rdf:resource="FeatureOfInterest"/>
      <owl:onProperty rdf:resource="isPropertyOf"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom rdf:resource="Device"/>
      <owl:onProperty rdf:resource="isMeasuredByDevice"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:allValuesFrom rdf:resource="Device"/>
      <owl:onProperty rdf:resource="isControlledByDevice"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:label xml:lang="en">Property</rdfs:label>
  <rdfs:comment xml:lang="en">A quality of a feature of interest that can be measured; an aspect of a feature of interest that is intrinsic to and cannot exist without the feature</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="Measurement">
